{
  "version": 2,
  "name": "moodflow-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_AUTH_MODE": "real"
  },
  "build": {
    "env": {
      "VITE_API_URL": "@vite_api_url",
      "VITE_AUTH_MODE": "real"
    }
  }
}